      "name": "4️⃣ Sales Records by Date Range",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Test: Check response status",
              "pm.test('Status code is 200', function() {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "// Test: Check date range fields",
              "pm.test('Response contains date range', function() {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData).to.have.property('startDate');",
              "    pm.expect(jsonData).to.have.property('endDate');",
              "});",
              "",
              "// Test: Check data array",
              "pm.test('Data is an array', function() {",
              "    var jsonData = pm.response.json();",
              "    pm.expect(jsonData.data).to.be.an('array');",
              "});",
              "",
              "// Test: All dates within range",
              "pm.test('All sales within date range', function() {",
              "    var jsonData = pm.response.json();",
              "    var startDate = new Date(jsonData.startDate);",
              "    var endDate = new Date(jsonData.endDate);",
              "    jsonData.data.forEach(function(record) {",
              "        var recordDate = new Date(record.sale_date);",
              "        pm.expect(recordDate.getTime()).to.be.greaterThanOrEqual(startDate.getTime());",
              "        pm.expect(recordDate.getTime()).to.be.lessThanOrEqual(endDate.getTime());",
              "    });",
              "});"
            ],
            "type": "text/javascript"
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Set date range for testing",
              "var today = new Date();",
              "var endDate = new Date(today.getFullYear(), today.getMonth(), today.getDate());",
              "var startDate = new Date(today.getFullYear(), today.getMonth(), today.getDate() - 30);",
              "",
              "var formatDate = function(date) {",
              "    return date.getFullYear() + '-' + String(date.getMonth() + 1).padStart(2, '0') + '-' + String(date.getDate()).padStart(2, '0');",
              "};",
              "",
              "if (!pm.collectionVariables.get('rangeStartDate')) {",
              "    pm.collectionVariables.set('rangeStartDate', formatDate(startDate));",
              "    pm.collectionVariables.set('rangeEndDate', formatDate(endDate));",
              "}",
              "console.log('Testing: Date Range ' + pm.collectionVariables.get('rangeStartDate') + ' to ' + pm.collectionVariables.get('rangeEndDate'));"
            ],
            "type": "text/javascript"
          }
        }
